{
  "name": "bd-estate",
  "version": "1.0.0",
  "description": "Real estate visualizer in D3 for the Terrace, British Columbia area",
  "main": "src/index.html",
  "dependencies": {},
  "devDependencies": {
    "@types/d3": "^4.7.0",
    "@types/handlebars": "^4.0.32",
    "@types/jquery": "^2.0.41",
    "ts-loader": "^2.0.1",
    "typescript": "^2.2.1",
    "webpack": "^2.2.1"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "ts": "tsc -p src",
    "dist:static": "copy src\\*.html dist\\ /Y && copy src\\*.css dist\\ /Y",
    "build:static": "copy src\\*.html build\\ /Y && copy src\\*.css build\\ /Y",
    "build": "npm run build:static && tsc --project src --sourceMap --outDir build",
    "dist": "npm run dist:static && tsc --project src",
    "watch": "tsc --project src --watch",
    "watch:build": "tsc --project src --watch --sourceMap --outDir build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/thebend/bd-estate.git"
  },
  "keywords": [
    "real",
    "estate",
    "d3",
    "terrace",
    "british",
    "columbia"
  ],
  "author": "Benjamin Davidson",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/thebend/bd-estate/issues"
  },
  "homepage": "https://github.com/thebend/bd-estate#readme"
}
